//Q. write a program to find the number of integers and strings in command line arguments
import java.lang.*;
import java.io.*;
class A
{
public static void main(String args[])throws IOException
{
int x;
float y;
int z;
int count1=0;
int count2=0;
int count3=0;



for(int i=0;i<args.length;i++)
{
System.out.println(args[i]);
try
{
x=Integer.parseInt(args[i]);
count1++;
y=Float.parseFloat(args[i]);
count3++;

}
catch(Exception e)
{
count2++;
}

}

System.out.println("the number of integers variables are ");
System.out.println(count1);
System.out.println("the number of string variables are ");
System.out.println(count2);
System.out.println("the number of float variables are ");
System.out.println(count3);

}
}
